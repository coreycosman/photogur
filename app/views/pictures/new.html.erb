<h1> Add a Picture </>
<%= form_for(@picture) do |f| %>
  <p>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %>
    <%= f.text_field :url %>
  </p>

  <p>
    <%= f.submit "save" %>
  </p>
<% end %>
<%= link_to("Back to Pictures", "/pictures") %>
